    "import pylev\n",
    "nlp = English()\n",
    "sent = \"I shot an elephant\"\n",
    "doc=nlp(sent)\n",
    "print(len(sent)-len(\"elephent\"))\n",
    "sub_toks=pylev.levenshtein(sent,\"elephent\")\n",
    "print(sub_toks)"
